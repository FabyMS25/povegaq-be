	<queryString>
		<![CDATA[select n.numero_notificacion, n.fecha_asistencia,n.hora_asistencia,n.es_denuncia,n.actividad,n.direccion,n.observacion, n.sancion,n.nombre_notificador, n.fecha_notificacion,
p.nombre || ' ' || 
p.primer_apellido || ' ' || 
  COALESCE(p.segundo_apellido) || ' ' || 
  COALESCE(p.apellido_esposo, '')  AS nombre_completo,p.numero_documento, n.numero_intento 
 ,v.placa, dt.color, dt.marca, dt.modelo, dt.clase, year_fabricacion, i.fecha_inspeccion, i.lugar_inspeccion 
from notificaciones as n
inner join inspecciones as i on i.id_inspeccion = n.id_inspeccion
inner join vehiculos as v on v.id_vehiculo = i.id_vehiculo
inner join datos_tecnicos as dt on dt.id_vehiculo = v.id_vehiculo
inner join propietarios as p on p.id_propietario = v.id_propietario
where n.uuid =$P{uuidNotificacion}
and n.estado = false
and i.estado = false
and v.estado = false
and p.estado = false
and dt.estado = false

union
select n.numero_notificacion, n.fecha_asistencia,n.hora_asistencia,n.es_denuncia,n.actividad,n.direccion,n.observacion, n.sancion,n.nombre_notificador, n.fecha_notificacion,
p.nombre || ' ' || 
p.primer_apellido || ' ' || 
  COALESCE(p.segundo_apellido) || ' ' || 
  COALESCE(p.apellido_esposo, '')  AS nombre_completo, p.numero_documento, n.numero_intento 
 ,v.placa, dt.color, dt.marca, dt.modelo, dt.clase, year_fabricacion, i.fecha_inspeccion, i.lugar_inspeccion 
 from notificaciones as n
inner join inspecciones as i on i.id_inspeccion = n.id_inspeccion
inner join vehiculos as v on v.id_vehiculo = i.id_vehiculo
inner join datos_tecnicos as dt on dt.id_vehiculo = v.id_vehiculo
inner join conductores as p on p.id_conductor = i.id_conductor
where n.uuid =$P{uuidNotificacion}
and n.estado = false
and i.estado = false
and v.estado = false
and p.estado = false
and dt.estado = false]]>
	</queryString>
