	<queryString>
		<![CDATA[SELECT i.estado_pago, i.fecha_infraccion, i.fecha_pago, i.monto_total, i.numero_tasa, i.status_infraccion, i.motivo,
COALESCE(v.placa, v.poliza, v.vin_numero_identificacion, v.pin_numero_identificacion) AS placa, 
i.nombre_registrador,a.tipo_actividad, t.grado
FROM infracciones as i 
inner join tipo_infracciones as t on t.id_tipo_infraccion = i.id_tipo_infraccion
inner join inspecciones as ip on ip.id_inspeccion = i.id_inspeccion	
inner join vehiculos as v on v.id_vehiculo = ip.id_vehiculo
inner join actividades as a on a.id_actividad = ip.id_actividad
where t.estado= false and a.estado = false and v.estado = false and ip.estado = false and i.estado = false 
and i.uuid = $P{uuidInfraccion}]]>
	</queryString>
