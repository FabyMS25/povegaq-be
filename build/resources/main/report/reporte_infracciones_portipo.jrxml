	<parameter name="valorUFV" class="java.math.BigDecimal"/>
	<queryString>
		<![CDATA[select v.placa, p.nombre || ' ' || p.primer_apellido || 
  COALESCE(' ' || p.segundo_apellido, '') AS nombre_completo, p.numero_documento, inf.motivo, inf.monto_total
  , ti.descripcion, ti.articulo, ti.grado, ti.valor_ufv
from infracciones as inf
inner join  tipo_infracciones as ti on ti.id_tipo_infraccion = inf.id_tipo_infraccion
inner join inspecciones as ins on ins.id_inspeccion = inf.id_inspeccion
inner join vehiculos as v on v.id_vehiculo = ins.id_vehiculo
inner join propietarios as p on p.id_propietario = v.id_propietario
where inf.estado = false and ti.estado = false and ins.estado = false
and v.estado= false and p.estado = false and inf.estado_pago = $P{estadoPago} 
AND inf.fecha_infraccion >= $P{fechaInicial}
AND inf.fecha_infraccion < $P{fechaFinal}
  AND ($P{descripcion} IS NULL OR ti.descripcion = $P{descripcion})
  AND ($P{articulo} IS NULL OR ti.articulo = $P{articulo})
  AND ($P{grado} IS NULL OR ti.grado = $P{grado})
  AND ($P{valorUFV} IS NULL OR ti.valor_ufv = $P{valorUFV})
union
select v.placa, c.nombre || ' ' || c.primer_apellido || 
  COALESCE(' ' || c.segundo_apellido, '') AS nombre_completo, c.numero_documento, inf.motivo, inf.monto_total
  , ti.descripcion, ti.articulo, ti.grado, ti.valor_ufv
from infracciones as inf
inner join tipo_infracciones as ti on ti.id_tipo_infraccion = inf.id_tipo_infraccion
inner join inspecciones as ins on ins.id_inspeccion = inf.id_inspeccion
inner join vehiculos as v on v.id_vehiculo = ins.id_vehiculo
inner join conductores as c on c.id_conductor = ins.id_conductor
where inf.estado = false and ti.estado = false and ins.estado = false
and v.estado= false and c.estado = false and inf.estado_pago = $P{estadoPago}
AND inf.fecha_infraccion >= $P{fechaInicial}
AND inf.fecha_infraccion < $P{fechaFinal}
  AND ($P{descripcion} IS NULL OR ti.descripcion = $P{descripcion})
  AND ($P{articulo} IS NULL OR ti.articulo = $P{articulo})
  AND ($P{grado} IS NULL OR ti.grado = $P{grado})
  AND ($P{valorUFV} IS NULL OR ti.valor_ufv = $P{valorUFV})]]>
	</queryString>
